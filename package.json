{
  "name": "eskiz-sms",
  "version": "1.1.16",
  "description": "Nodejs package for eskiz.uz SMS service",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p tsconfig.build.json",
    "type-check": "tsc -p tsconfig.build.json --noEmit",
    "test": "jest",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "prepare": "husky"
  },
  "keywords": [
    "eskiz",
    "sms",
    "nodejs",
    "typescript"
  ],
  "author": "Manuchehr Raupov",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@release-it/conventional-changelog": "^10.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.2",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.5.0",
    "release-it": "^18.1.2",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "dotenv": "^16.4.7"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./test",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mdotme/node-eskiz-sms.git"
  },
  "bugs": {
    "url": "https://github.com/mdotme/node-eskiz-sms/issues"
  },
  "homepage": "https://github.com/mdotme/node-eskiz-sms#readme"
}
