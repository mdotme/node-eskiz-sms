{
  "git": {
    "requireCleanWorkingDir": true,
    "commit": true,
    "commitMessage": "chore: release v${version}",
    "tag": true,
    "tagName": "v${version}",
    "push": true,
    "pushRepo": "https://github.com/mdotme/node-eskiz-sms.git"
  },
  "github": {
    "release": true,
    "releaseName": "Release v${version}"
  },
  "npm": {
    "publish": true
  },
  "hooks": {
    "after:release": "echo Successfully released v${version}!"
  }
}
